    "dev": "WEBPACK_ENV=dev webpack-dev-server",
    "lint": "eslint ./src/**/*.js /test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videoanalyticsio/videoanalytics.io.git"
  },
  "keywords": [
    "video analytics",
    "html5 video",
    "video",
    "analytics"
  ],
  "author": "Adam Stankiewicz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/videoanalyticsio/videoanalytics.io/issues"
  },
  "homepage": "https://github.com/videoanalyticsio/videoanalytics.io#readme",
  "engines": {
    "node": ">=5.12.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.14",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "fs": "^0.0.1-security",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "unminified-webpack-plugin": "^1.1.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {
    "lodash": "^4.17.2"
  }
