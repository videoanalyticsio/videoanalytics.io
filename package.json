{
  "name": "videoanalytics.io",
  "version": "0.0.5",
  "description": "HTML5 video analytics",
  "main": "./dist/index.js",
  "scripts": {
    "test": "babel-istanbul cover _mocha -- -r jsdom-global/register --colors --recursive --reporter spec",
    "coverage": "[ -f ./coverage/lcov.info ] && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || exit 1",
    "build": "WEBPACK_ENV=build webpack",
    "prepublish": "npm run build",
    "dev": "WEBPACK_ENV=dev webpack --watch & webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamstankiewicz/videoanalytics.io.git"
  },
  "keywords": [
    "video analytics",
    "html5 video",
    "video",
    "analytics"
  ],
  "author": "Adam Stankiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamstankiewicz/videoanalytics.io/issues"
  },
  "homepage": "https://github.com/adamstankiewicz/videoanalytics.io#readme",
  "engines": {
    "node": ">=4.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.0",
    "eslint-loader": "^1.6.1",
    "html-webpack-plugin": "^2.24.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.17.2"
  }
}
